.mapWrap {
  height: 100vh;
}

.mapWrap map {
  width: 100%;
  height: 100%;
}

.mapWrap .btn {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 6vh;
  background-color: rgba(0, 0, 0, 0.1);
  display: flex;
}

.mapWrap .btn .con {
  flex: 1;
  height: 100%;
  text-align: center;
  line-height: 6vh;
}
